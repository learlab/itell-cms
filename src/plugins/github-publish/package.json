{
  "name": "github-publish",
  "version": "0.0.0",
  "description": "A plugin to easily trigger GitHub Actions workflows from Strapi.",
  "strapi": {
    "name": "github-publish",
    "description": "Description of Github Publish plugin",
    "kind": "plugin",
    "displayName": "Github Publish"
  },
  "dependencies": {
    "@strapi/design-system": "^1.6.3",
    "@strapi/helper-plugin": "^4.6.0",
    "@strapi/icons": "^1.6.3",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1"
  },
  "scripts": {
    "lint": "NODE_ENV=test npm-run-all --parallel lint:*",
    "lint:js": "eslint \"**/*.{js,jsx}\"",
    "lint:format": "prettier \"**/*.{md,html,yaml,yml,json}\" --check",
