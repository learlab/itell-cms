{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Pages",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Content": {
      "type": "dynamiczone",
      "components": [
        "page.video",
        "page.chunk",
        "page.plain-chunk"
      ]
    },
    "Slug": {
      "type": "uid",
      "targetField": "Title",
      "required": true
    },
    "HasSummary": {
      "type": "boolean",
      "default": true
    },
    "Quiz": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::quiz.quiz",
      "mappedBy": "PreviousPage"
    },
    "ReferenceSummary": {
      "type": "text"
    },
    "Chapter": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::chapter.chapter",
      "inversedBy": "Pages"
    },
    "Volume": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::text.text",
      "inversedBy": "Pages"
    }
  }
}
